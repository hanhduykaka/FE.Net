

@{
    Layout = "~/DesktopModules/MVC/SoYTe.GUI/TrangChuModule/Views/Shared/_Layout.cshtml";
}

<div class="syt-content mt-85">
    <div class="container nopadding">
        <div class="syt-content-div-title paddingMin">
            <p>
                <span class="ml-5 sknadTitle-white font-size-16">
                    Ngành dược &nbsp; &nbsp;
                    <span class="glyphicon glyphicon-triangle-right font-size-9"></span>
                    &nbsp; &nbsp;
                    Chứng chỉ hành nghề dược
                    <span class="glyphicon glyphicon-triangle-right font-size-9"></span>
                    &nbsp; &nbsp; Danh sách chứng chỉ
                </span>
            </p>
        </div>

        <div class="syt-content-div-box-search">
            <div class="sytClearFix-19 dnnClear "></div>
            <div class="sytClearFix-min dnnClear "></div>
            <div class="col-md-12 mt-10">
                <div class="col-md-4">
                    <div class="row">
                        <div class="col-md-5  text-right  syt-label">
                            <span class="sknadTitle-gray-normal font-size-13"> Số chứng chỉ </span>
                        </div>
                        <div class="col-md-7 nopadding">
                            <input class="width-100 syt-radius-3 min-h-30 paddingMin" name="SoChungChi" type="text" />
                        </div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6  text-right  syt-label">
                                    <span class="sknadTitle-gray-normal font-size-13"> Ngày nhận từ </span>
                                </div>
                                <div class="col-md-6 nopadding">
                                    <input class="width-100 syt-radius-3 min-h-30 paddingMin nopadding" name="NgayNhanTu" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6  text-right  syt-label">
                                    <span class="sknadTitle-gray-normal font-size-13"> đến </span>
                                </div>
                                <div class="col-md-6 nopadding">
                                    <input class="width-100 syt-radius-3 min-h-30 paddingMin nopadding" name="NgayNhanDen" type="text" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="sytClearFix-min dnnClear "></div>
            <div class="col-md-12 mt-10">
                <div class="col-md-4">
                    <div class="row">
                        <div class="col-md-5  text-right  syt-label">
                            <span class="sknadTitle-gray-normal font-size-13"> Người nộp </span>
                        </div>
                        <div class="col-md-7 nopadding">
                            <input class="width-100 syt-radius-3 min-h-30 paddingMin" name="NguoiNop" type="text" />
                        </div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="row">
                        <div class="col-md-3  text-right  syt-label">
                            <span class="sknadTitle-gray-normal font-size-13"> Số giấy tờ </span>
                        </div>
                        <div class="col-md-3 nopadding">
                            <input class="width-100 syt-radius-3 min-h-30 paddingMin" name="SoGiayTo" type="text" />
                        </div>
                        <div class="col-md-3  text-right  syt-label">
                            <span class="sknadTitle-gray-normal font-size-13"> Trạng thái</span>
                        </div>
                        <div class="col-md-3 nopadding">
                            <select class="sknadSelect width-100 adselect" name="TrangThai"></select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="syt-content-div-icon-search">
            <div class="sytClearFix-19 dnnClear "></div>
            <div class="col-md-12">
                <div class="col-md-6"></div>
                <div class="col-md-6">
                    <div class="row">
                        <div class="col-md-2 paddingMin">
                            <input class="cursor-pointer sytButton sknadTitle-white width-100 btn_search"
                                   type="button" value="Tìm" />
                        </div>
                        <div class="col-md-3 paddingMin">
                            <input class="cursor-pointer sytButton sknadTitle-white width-100 btn_nhap_dau_ky"
                                   type="button" value="Nhập đầu kỳ" />
                        </div>
                        <div class="col-md-4 paddingMin">
                            <input type="button" class="cursor-pointer sknadButtonExport width-100 font-weight-700 btn_xuat_danh_sach"
                                   value="Xuất danh sách" />
                        </div>
                        <div class="col-md-3 paddingMin">
                            <input type="button" class="cursor-pointer sknadButtonExport width-100 font-weight-700 btn_cong_bo_website"
                                   value="Công bố website" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="div_results">
            <table width="100%" class="table table-striped table-data" id="tbl_danh_sach_chung_chi">
                <thead>
                    <tr>
                        <th class="text text-center bgth font-size-13">STT</th>
                        <th class="text text-center bgth font-size-13">Số CC</th>
                        <th class="text text-center bgth font-size-13"> Ngày cấp </th>
                        <th class="text text-center bgth font-size-13">Họ và tên</th>
                        <th class="text text-center bgth font-size-13">Số giấy tờ</th>
                        <th class="text text-left bgth  font-size-13 ">Địa chỉ</th>
                        <th class="text text-center bgth  font-size-13 ">Phạm vi hành nghề</th>
                        <th class="text text-center bgth  font-size-13 ">Trạng thái</th>
                        <th class="text text-center bgth  font-size-13 ">Xử lý</th>
                    </tr>
                </thead>
                <tbody class="font-size-13 font-weight-400">
                   
                </tbody>
            </table>
        </div>

    </div>
</div>
<script>
    var rvtoken = $("input[name='__RequestVerificationToken']").val();
    var moduleId = @Dnn.ModuleContext.ModuleId;
    var tabId = @Dnn.ModuleContext.TabId;
</script>

<div class="sytClearFix dnnClear"></div>

<script src="~/DesktopModules/MVC/SoYTe.GUI/NghiepVuModule/Script/NganhDuoc/nganhduoc.index.ccduoc.js"></script>
